vagetables = {'картофанчик':50 ,'морковачка':40, 'огурчик':35, 'перчик':30, 'баклажанчик':55}
a = ['картофанчик', 'морковачка', 'огурчик', 'перчик', 'баклажанчик']
a.append('редиска')
a.pop(0)
a.sort()
p = input()
f = int(input())
s = {p:f}
vagetables.update(s)
for i, j in vagetables.items():
    print(i, 'стоит', j, 'рублей')


